{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "libs",
  "projects": {
    "search": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/search",
      "sourceRoot": "apps/search/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/search",
            "index": "apps/search/src/index.html",
            "main": "apps/search/src/main.ts",
            "polyfills": "apps/search/src/polyfills.ts",
            "tsConfig": "apps/search/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/search/src/favicon.ico",
              "apps/search/src/assets"
            ],
            "styles": [
              "apps/search/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/search/src/environments/environment.ts",
                  "with": "apps/search/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "search:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "search:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "search:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "assets": [
              "apps/search/src/favicon.ico",
              "apps/search/src/assets"
            ],
            "styles": [
              "apps/search/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/search/tsconfig.app.json",
              "apps/search/tsconfig.spec.json",
              "apps/search/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/search/e2e/protractor.conf.js",
            "devServerTarget": "search:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "search:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "search"
}
